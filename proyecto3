def bu():
    text= fila1.get()
    semilla ="1"
    if text == str(semilla):
        print(semilla)

    else:
        print("dddd")

        


def jefe():

    conta =0
    
    a=n2.get()

    for e in a :
        conta = conta +1 
        print(e)

    if conta > 20:
        v2 =Tk()
        v2.config(bg = "GREY")
        v2.geometry("370x30")
        infoerror=Label(v2 , text = "ERROR : EL NOMBRE DEBE SER MENOR A 20 CARACTERES")

        infoerror.grid(row=3 , column=2)

    if conta == 0:
        v2 =Tk()
        v2.config(bg = "GREY")
        v2.geometry("370x30")
        infoerror=Label(v2 , text = "ERROR : INTRODUCE TU NOMBRE ")

        infoerror.grid(row=3 , column=2)
        

         
    semilla=0

    return 
    
    




