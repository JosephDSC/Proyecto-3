def opciones2(fila1):

    f = fila1 
    
    iniciarjuego= Button(ventana , text ="INICAR JUEGO" , bg = "RED" , command= iniciar)
    iniciarjuego.grid(row=14 , column=0)

    borrarjugada =Button(ventana , text = "BORRAR JUGADA"  , bg= "BLUE")
    borrarjugada.grid(row=14 , column=1)

    terminarjuego=Button(ventana , text="TERMINAR JUEGO" , bg ="GREEN" )
    terminarjuego.grid(row=14 , column=2)

    borrarjuego=Button(ventana , text ="BORRAR JUEGO" , bg ="GREY")
    borrarjuego.grid(row=14 , column=3)


    top=Button(ventana , text = "TOP" , bg= "YELLOW")
    top.grid(row=14 , column=4)

    top1=Button(ventana , text = "  AMOR" , bg= "YELLOW")
    top1.grid(row=14 , column=4)
    
    ventana.mainloop()



#CONFIGURACION DE LA VENTANA
    
ventana=Tk()
ventana.geometry("600x600")
ventana.config(bg="black")
titulo= Label(ventana , text= "FUTOSHIKI"  , bg ="RED")
titulo.grid(row=1 , column=3)

acti = [1]

#TEXTO DE NOMBRE DE JUGADOR
nombre= Label(ventana , text="Nombre de jugador:")
nombre.grid(row=2 , column=0)

#COLOCAR EL NOMBRE
n2=Entry(ventana ,  width = 20  )
n2.grid(row=2 , column=1)


#BOTON PARA GUARDAR
c = Button (ventana , text="ACEPTAR" , command=jefe )

c.grid(row=3 , column=0)
nombre=n2.get()
print(nombre)

acti=[1]
opciones(acti)

ventana.mainloop()




    



